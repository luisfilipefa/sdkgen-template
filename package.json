    "bcrypt": "^5.0.1",
    "class-transformer": "^0.4.0",
    "dotenv": "^10.0.0",
    "knex": "^0.95.11",
    "pg": "^8.7.1",
    "ts-node": "^10.2.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/faker": "^5.5.8",
    "@types/knex": "^0.16.1",
    "@types/node": "^16.9.4",
    "@types/uuid": "^8.3.1",
    "faker": "^5.5.3",
    "typescript": "^4.4.3"
  }
}
