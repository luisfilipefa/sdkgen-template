{
  "name": "sdkgen-template",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "sdkgen": "sdkgen src/schemas/index.sdkgen -o src/generated/generated.ts -t typescript_nodeserver",
